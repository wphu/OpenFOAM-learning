        type            calculated;
        value           uniform 0;
    }
    wall
    {
        type            calculated;
        value           uniform 0;
    }
    side_x0
    {
        type            cyclic;
    }
    side_x1
    {
        type            cyclic;
    }
    side_y0
    {
        type            cyclic;
    }
    side_y1
    {
        type            cyclic;
    }
